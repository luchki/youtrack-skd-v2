{
    "name": "luchki/youtrack-sdk",
    "type": "library",
    "description": "SDK для Youtrack, позволяет работать с API youtrack более удобным способом",
    "keywords": [
        "php",
        "sdk",
        "Youtrack",
        "API"
    ],
    "homepage": "https://github.com/luchki/youtrack-skd-v2",
    "authors": [
        {"name": "George Kiknadze", "email": "zinguru@gmail.com"}
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.4",
        "doctrine/collections": "^1.6",
        "vlucas/phpdotenv": "^5.3"
    },
    "require-dev": {
        "ext-json": "*",
        "ext-http": "*",
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.0.0"
    },
    "autoload": {
        "psr-4": { "Luchki\\YoutrackSDK\\": "src/"}
    }
